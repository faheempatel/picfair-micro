<p id="notice"><%= notice %></p>

<h1>Listing Pics</h1>

  <% @pics.each do |pic| %>
    <h3><%= pic.name %></h3>
    <p><%= pic.description %></p>
    <p>Starred: <%= pic.starred %></p>
    <%= link_to image_tag(pic.url, :alt => pic.description), pic %>
  <% end %>

<br>

<%= link_to 'Upload', upload_path %>
